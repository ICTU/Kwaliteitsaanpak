{
    "BuildPath": "build/Templates/NFE/",
    "Title": "Niet-Functionele Eisen",
    "Subtitle": "{Productnaam, versie}",
    "DocumentType": "Template",
    "DocumentFolder": "NFE",
    "FrontPage": "Neutral",
    "OutputFormats": {
        "docx": {
            "IncludeTableOfContents": true,
            "InputFile": "DocumentDefinitions/Templates/document-template.md",
            "OutputFile": "Neutraal-Template-Niet-Functionele-Eisen.docx",
            "OutputPaths": [
                "docs/$VERSIE$"
            ],
            "ReferenceFile": "DocumentDefinitions/reference-neutral.docx"
        },
        "xlsx": {
            "IncludeTableOfContents": false,
            "InputFile": "DocumentDefinitions/Templates/spreadsheet-template.md",
            "BuilderClass": "TableXlsxBuilder",
            "OutputFile": "Neutraal-Template-Niet-Functionele-Eisen.xlsx",
            "OutputPaths": [
                "docs/$VERSIE$"
            ]
        }
    },
    "VariablesFiles": [
        "DocumentDefinitions/Shared/variables.json"
    ]
}
